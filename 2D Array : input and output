#include<iostream>
using namespace std;

int main()
{
    int mat[4][3];
    int rows=4;
    int cols=3;
    
    //Input
    cout<<"Enter 2D Array numbers: "<<endl;
    for(int i=0;i<rows;i++)
    {
            for(int j=0;j<cols;j++)
            {
                cin>>mat[i][j];
            }

    }
    
    //output
    cout<<"2D Array numbers are: "<<endl;
    for(int i=0;i<rows;i++)
    {
            for(int j=0;j<cols;j++)
            {
                cout<<mat[i][j]<<" ";
            }

        cout<<endl;
    }
}
