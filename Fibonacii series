
#include<iostream>
using namespace std;

int main()
{
    int n;
    cout<<"Enter value of n:";
    cin>>n;
    
    int a=0;
    int b=1;
    int next;
    
    cout<<"Fibonacii series upto "<<n<<" number: ";
    for(int i=0;i<n;i++)
    {
        cout<<a<<" ";
        next=a+b;
        a=b;
        b=next;
    }
}
